{
  "name": "d3-svg-annotation",
  "version": "0.0.0",
  "description": "",
  "main": "d3-annotation.js",
  "jsnext:main": "d3-annotation.mjs",
  "files": [
    "lib",
    "index.js",
    "d3-annotation.*"
  ],
  "keywords": [],
  "directories": {},
  "devDependencies": {
    "babel-plugin-external-helpers": "6.18.0",
    "babel-plugin-transform-object-rest-spread": "6.20.2",
    "babel-preset-es2015": "6.18.0",
    "babelify": "7.3.0",
    "babelrc-rollup": "3.0.0",
    "browserify": "13.1.1",
    "http-server": "0.9.0",
    "marked": "0.3.6",
    "markedify": "0.0.2",
    "npm-watch": "0.1.6",
    "rollup": "0.34.7",
    "rollup-plugin-babel": "2.6.1"
  },
  "scripts": {
    "start": "npm run server & npm run watch",
    "server": "http-server",
    "browserify": "browserify web.js -o d3-annotation.js -t [ babelify --presets [ es2015 ] ]",
    "docs-min": "browserify web.js -o docs/d3-annotation.min.js -t [ babelify --presets [ es2015 ] ]",
    "docs": "browserify docs/docs.js -o docs/docs.min.js -t [ markedify babelify --presets [ es2015 ] ]",
    "watch": "npm-watch"
  },
  "watch": {
    "browserify": "src/*.js",
    "docs": {
      "patterns": ["docs/docs.js", "docs/content/*.md"],
      "extensions": "js,md"
    },
    "docs-min": "src/*.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/susielu/d3-annotation.git"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/susielu/d3-annotation/issues"
  },
  "dependencies": {
    "d3-drag": "1.0.2",
    "d3-selection": "1.0.2",
    "d3-shape": "1.0.4",
    "d3-transition": "1.0.3"
  }
}
